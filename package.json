{
  "name": "jobs-server",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node src/index.js",
    "docker": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml",
    "docker:build-dev": "npm run docker -- build",
    "docker:up": "npm run docker -- up",
    "docker:down": "npm run docker -- down",
    "docker:build": "docker-compose -f docker/docker-compose.yml build",
    "docker:prod": "docker-compose -f docker/docker-compose.yml up -d",
    "test": "npm run lint && ava src/**/*.test.js",
    "lint": "standard src/**/*.mjs"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "prettier": "^1.17.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "@sentry/node": "5.1.0",
    "amqplib": "^0.5.3",
    "dotenv": "^7.0.0",
    "knex": "^0.16.5",
    "pg": "^7.10.0",
    "redis": "^2.8.0"
  }
}
